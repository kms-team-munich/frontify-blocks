{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "jsx": "react-jsx",
    "lib": ["ES2021", "DOM"],
    "resolveJsonModule": true,
    "types": ["node", "cypress"],
    "skipLibCheck": true
  }
}
