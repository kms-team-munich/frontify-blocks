{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "sourceMap": true,
    "strict": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "types": [
      "node",
      "cypress",
      "cypress-real-events",
      "@4tw/cypress-drag-drop"
    ]
  },
  "exclude": ["**/dist", "**/node_modules"]
}
